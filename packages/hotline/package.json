{
  "name": "@memento/hotline",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Websocket management for memento",
  "main": "es5/index.js",
  "types": "es5/index.d.ts",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "prepublish": "tsc --p ./tsconfig.publish.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "flux",
    "state management"
  ],
  "author": "jeanfortheweb",
  "devDependencies": {
    "@memento/probe": "0.0.1",
    "@types/jest": "22.2.2",
    "@types/node": "9.6.0",
    "jest": "22.4.3",
    "ts-jest": "22.4.2",
    "typescript": "2.7.2",
    "ws": "5.1.0"
  },
  "dependencies": {
    "@memento/store": "0.0.1",
    "@reactivex/rxjs": "5.5.7",
    "immutable": "4.0.0-rc.9"
  },
  "jest": {
    "bail": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "\\.spec\\.ts$"
  }
}
