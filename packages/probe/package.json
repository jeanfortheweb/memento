{
  "name": "@memento/probe",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Worker and store testing utilities for memento",
  "main": "es5/index.js",
  "types": "es5/index.d.ts",
  "scripts": {
    "test": "echo 'No tests yet'",
    "coverage": "echo 'No tests yet'",
    "prepublish": "tsc --p ./tsconfig.publish.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "flux",
    "state management"
  ],
  "author": "jeanfortheweb",
  "devDependencies": {
    "@types/jest": "22.2.2",
    "jest": "22.4.3",
    "ts-jest": "22.4.2",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "@memento/store": "0.0.1",
    "@reactivex/rxjs": "5.5.7",
    "@types/faker": "4.1.2",
    "faker": "4.1.0",
    "immutable": "4.0.0-rc.9"
  },
  "jest": {
    "bail": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "\\.spec\\.ts$",
    "mapCoverage": true
  }
}
